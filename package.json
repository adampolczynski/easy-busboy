{
  "name": "easy-busboy",
  "package-name": "easy-busboy",
  "version": "0.0.0",
  "displayName": "Easy Busboy wrapper",
  "description": "Await-like simple Busboy wrapper for express 4, 5, Koa, also works within some cloud functions",
  "keywords": [
    "easy-busboy",
    "easy-upload",
    "busboy",
    "wrapper",
    "upload",
    "multi-upload",
    "form",
    "Express",
    "Koa",
    "middleware",
    "file-upload",
    "async",
    "await"
  ],
  "main": "./lib/index.ts",
  "author": {
    "name": "Adam Połczyński <adam_polczynski@yahoo.com>"
  },
  "license": "MIT",
  "scripts": {
    "start:server:express4": "tsx watch server/express4/index.ts",
    "start:server:express5": "tsx watch server/express5/index.ts",
    "start:server:koa": "tsx watch server/koa/index.ts",
    "start:upload": "tsx watch client/send-file.ts"
  },
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/express": "^5.0.0",
